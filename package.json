{
  "name": "visalex-market",
  "version": "1.0.0",
  "scripts": {
    "watch": "npm run clean:dist && webpack --watch --progress",
    "watch:dev": "npm run server & npm run browsersync & npm run watch",
    "clean:dist": "rimraf dist",
    "clean:ngc": "rimraf **/*.ngfactory.ts **/*.css.shim.ts",
    "prebuild": "npm run clean:dist",
    "build:qa": "npm run clean:dist && cross-env API_URL=apiQa webpack --config webpack.prod.jit.config.ts",
    "build:stage": "npm run clean:dist && cross-env API_URL=apiStage webpack --config webpack.prod.jit.config.ts",
    "build:dev": "npm run clean:dist && webpack  --progress",
    "build:prod:ngc": "npm run clean:ngc && npm run ngc && npm run clean:dist && npm run build:prod",
    "build:prod:ngc:json": "npm run clean:ngc && npm run ngc && npm run clean:dist && npm run build:prod:json",
    "build:prod": "webpack --config webpack.prod.config.ts",
    "build:prod:jit": "webpack --config webpack.prod.jit.config.ts --progress",
    "build:prod:json": "webpack --config webpack.prod.config.ts --json | webpack-bundle-size-analyzer",
    "ngc": "ngc -p tsconfig.aot.json",
    "server": "nodemon dist/server/index.js",
    "debug:server": "node-nightly --inspect --debug-brk dist/server/index.js",
    "prestart": "npm run build:qa",
    "start": "npm run server",
    "debug:start": "npm run build && npm run debug:server",
    "predebug": "npm run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug:build:prod": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js  --config webpack.prod.config.ts",
    "debug": "node --debug-brk dist/server/index.js",
    "browsersync": "browser-sync start --p 'localhost:3000' --port 3100 --reload-delay 3000 --no-open"
  },
  "contributors": [
    "Ivan Matiishyn <matiyishyn.ivan@gmail.com>"
  ],
  "dependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/compiler-cli": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/http": "~2.1.2",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "@angular/platform-server": "~2.1.2",
    "@angular/router": "~3.1.2",
    "@angular/upgrade": "~2.1.2",
    "@angularclass/bootloader": "~1.0.1",
    "@angularclass/idle-preload": "~1.0.4",
    "@ngtools/webpack": "~1.1.7",
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/fb": "0.0.22",
    "@types/gapi": "0.0.30",
    "@types/gapi.auth2": "0.0.32",
    "@types/hammerjs": "^2.0.32",
    "@types/memory-cache": "0.0.29",
    "@types/mime": "0.0.28",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.38",
    "@types/serve-static": "^1.7.27",
    "@types/webfontloader": "^1.6.27",
    "accepts": "^1.3.3",
    "angular2-express-engine": "~2.1.0-rc.1",
    "angular2-platform-node": "~2.1.0-rc.1",
    "angular2-universal": "~2.1.0-rc.1",
    "angular2-universal-polyfills": "~2.1.0-rc.1",
    "body-parser": "^1.15.2",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "express-interceptor": "^1.2.0",
    "http-proxy-middleware": "^0.17.3",
    "iltorb": "^1.0.13",
    "js.clone": "0.0.3",
    "jstz": "^1.0.8",
    "memory-cache": "^0.1.6",
    "methods": "~1.1.2",
    "morgan": "^1.7.0",
    "ng2-translate": "^4.2.0",
    "nodemon": "^1.10.0",
    "preboot": "~4.5.2",
    "reflect-metadata": "0.1.8",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.1.9",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "ssl-root-cas": "^1.2.3",
    "string-replace-loader": "^1.1.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "^3.0.2",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webfontloader": "^1.6.27",
    "webpack": "2.1.0-beta.27",
    "webpack-bundle-analyzer": "1.4.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-merge": "~0.16.0"
  }
}
